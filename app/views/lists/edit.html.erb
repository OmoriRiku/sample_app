<h1>編集画面</h1>

<%= form_with model: @list, url: list_path(@list.id), method: :patch do |f| %>
    <h2>タイトル</h2>
    <%= f.text_field :title %>
    
    <h2>本文</h2>
    <%= f.text_area :body %>
    
    <h2>画像</h2>
    <p><%= f.file_field :image, accept: 'image/*'%></p>
    
    <p><%= f.submit '投稿' %></p>
    
<% end %>